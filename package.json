{
  "name": "warframe-status",
  "version": "1.10.14",
  "author": "tobitenno",
  "repository": "https://github.com/wfcd/warframe-status.git",
  "private": true,
  "dependencies": {
    "@sentry/node": "^7.80.0",
    "@sentry/tracing": "^7.80.0",
    "@wfcd/arsenal-parser": "^1.2.3",
    "apicache": "^1.6.3",
    "colors": "1.4.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-favicon-short-circuit": "^1.1.1",
    "flat-cache": "^3.0.4",
    "helmet": "^7.1.0",
    "json-fetch-cache": "^1.2.6",
    "node-fetch": "^2.7.0",
    "swagger-stats": "^0.99.7",
    "warframe-items": "^1.1260.166",
    "warframe-nexus-query": "^1.7.1",
    "warframe-worldstate-data": "^1.28.45",
    "warframe-worldstate-parser": "^2.31.0",
    "winston": "^3.11.0",
    "worldstate-emitter": "^1.0.8",
    "ws": "^7.5.9"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "prom-client": "^11.5.3",
    "utf-8-validate": "^5.0.10"
  },
  "devDependencies": {
    "@sentry/types": "^7.80.0",
    "@types/apicache": "^1.6.5",
    "@types/cors": "^2.8.16",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.21",
    "@types/flat-cache": "^2.0.0",
    "@types/helmet": "4.0.0",
    "@types/twitter": "^1.7.4",
    "@types/ws": "^8.5.3",
    "@wfcd/eslint-config": "^1.4.1",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "install-peerdeps": "^2.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0"
  },
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon",
    "dev:pm2": "pm2 start warframe-status.json",
    "lint": "eslint src/ --ext .js,.mjs",
    "lint:fix": "eslint src/ --fix --ext .js,.mjs",
    "test": "nyc mocha",
    "update:spec": "curl --url https://docs.warframestat.us/openapi.json -G -o src/api-spec/openapi.json --silent",
    "hydrate:cache": "BUILD=build WFINFO_FILTERED_ITEMS=$WFINFO_FILTERED_ITEMS WFINFO_PRICES=$WFINFO_PRICES node ./src/lib/hydrate.js",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "build": "npm run hydrate:cache",
    "postinstall": "install-peerdeps @wfcd/eslint-config@^1.1.4 -S"
  },
  "engines": {
    "node": ">=16.0.0 < 17",
    "npm": ">=8.0.0 < 9"
  },
  "license": "Apache-2.0",
  "nodemonConfig": {
    "ignore": [
      "src/spec/**",
      "pacakge*.json",
      "src/api-spec/**"
    ],
    "env": {
      "LOG_LEVEL": "info",
      "API_BASE_URL": "http://localhost:3001",
      "NODE_ENV": "development",
      "PORT": 3001,
      "CACHE_TIMEOUT": 30000,
      "DISABLE_PRICECHECKS": "true",
      "USER_AGENT": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/89.0.4389.114 Safari/537.36"
    }
  },
  "eslintIgnore": [
    "src/lib/generate.js"
  ],
  "eslintConfig": {
    "extends": [
      "@wfcd"
    ],
    "rules": {
      "no-underscore-dangle": "off",
      "no-restricted-syntax": "off"
    }
  },
  "prettier": "@wfcd/eslint-config/prettier",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/exec",
        {
          "successCmd": "echo 'yes' >> .release",
          "failCmd": "echo 'no' >> .release"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ]
        }
      ],
      "@eclass/semantic-release-sentry-releases"
    ],
    "branches": "main"
  },
  "mocha": {
    "exit": true,
    "spec": "src/spec/**/*.spec.*js",
    "enableSourceMaps": true,
    "enable-source-maps": true,
    "checkLeaks": true,
    "timeout": 60000,
    "normal": 500,
    "slow": 2000,
    "fullTrace": true,
    "bail": true,
    "require": [
      "src/spec/hooks/env.js",
      "src/spec/hooks/start.hook.js"
    ]
  },
  "nyc": {
    "exclude": [
      "src/spec/**",
      "src/controllers/index.js",
      "src/lib/*"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "source-map": true,
    "sourceMap": true,
    "produce-source-map": true,
    "skip-full": true
  }
}
