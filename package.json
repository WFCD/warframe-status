{
  "name": "warframe-status",
  "version": "2.6.25",
  "private": true,
  "repository": "https://github.com/wfcd/warframe-status.git",
  "license": "Apache-2.0",
  "author": "tobitenno",
  "type": "module",
  "scripts": {
    "build": "npm run hydrate:cache",
    "dev": "nodemon",
    "dev:pm2": "pm2 start warframe-status.json",
    "hydrate:cache": "BUILD=build WFINFO_FILTERED_ITEMS=$WFINFO_FILTERED_ITEMS WFINFO_PRICES=$WFINFO_PRICES node ./src/lib/hydrate.js",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepare": "husky",
    "start": "node main.js",
    "test": "cross-env HOSTNAME=localhost c8 mocha",
    "update:spec": "curl --url https://docs.warframestat.us/openapi.json -G -o src/api-spec/openapi.json --silent"
  },
  "dependencies": {
    "@sentry/node": "^10.32.1",
    "@wfcd/arsenal-parser": "^2.0.6",
    "@wfcd/items": "^1.1272.116",
    "@wfcd/profile-parser": "^2.0.11",
    "apicache": "^1.6.3",
    "colors": "1.4.0",
    "cors": "^2.8.6",
    "cron": "^4.4.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-async-handler": "^1.2.0",
    "express-favicon-short-circuit": "^1.2.0",
    "flat-cache": "^6.1.20",
    "helmet": "^8.1.0",
    "ip-address": "^10.1.0",
    "json-fetch-cache": "^1.2.8",
    "json5": "^2.2.3",
    "node-fetch": "^3.3.2",
    "swagger-stats": "^0.99.7",
    "warframe-nexus-query": "^2.2.2",
    "warframe-worldstate-data": "^3.2.1",
    "warframe-worldstate-parser": "^5.2.16",
    "winston": "^3.19.0",
    "worldstate-emitter": "^2.2.13",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@sentry/types": "^10.32.1",
    "@types/apicache": "^1.6.8",
    "@types/chai-http": "^4.2.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/flat-cache": "^2.0.2",
    "@types/helmet": "4.0.0",
    "@types/twitter": "^1.7.4",
    "@types/ws": "^8.18.1",
    "c8": "^10.1.3",
    "chai": "^6.2.2",
    "chai-http": "^5.1.2",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.11"
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "prom-client": "^11.5.3",
    "utf-8-validate": "^6.0.6"
  }
}
