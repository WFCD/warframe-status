{
  "name": "warframe-status",
  "version": "1.0.2",
  "author": "tobitenno",
  "repository": "https://github.com/wfcd/warframe-status.git",
  "dependencies": {
    "@sentry/node": "^5.20.1",
    "apicache": "^1.5.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.3",
    "express-favicon-short-circuit": "^1.1.1",
    "helmet": "^3.23.3",
    "json-fetch-cache": "^1.2.6",
    "nexushub-client": "^1.2.0",
    "swagger-stats": "^0.95.11",
    "warframe-items": "^1.1099.0",
    "warframe-nexus-query": "^1.6.13",
    "warframe-worldstate-data": "^1.8.1",
    "warframe-worldstate-parser": "^2.15.0",
    "winston": "^3.3.3",
    "worldstate-emitter": "^1.0.4",
    "ws": "^7.3.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@sentry/types": "^5.20.1",
    "@types/apicache": "^1.2.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.43",
    "@types/socket.io": "^2.1.10",
    "@types/twitter": "^1.7.0",
    "@types/ws": "^7.2.6",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.22.0",
    "greenkeeper-lockfile": "^1.15.1",
    "nodemon": "^2.0.4"
  },
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon",
    "dev:pm2": "pm2 start warframe-status.json",
    "lint": "npx eslint src/",
    "lint:fix": "npx eslint src/ --fix",
    "test": "exit 0",
    "snyk-protect": "npx snyk protect",
    "prepare": "npm run snyk-protect",
    "update:spec": "curl --url https://docs.warframestat.us/openapi.json -G -o src/api-spec/openapi.json --silent"
  },
  "engines": {
    "node": "11.15.0",
    "npm": "6.7.0"
  },
  "license": "Apache-2.0",
  "nodemonConfig": {
    "ignore": [
      "**/test/**.**"
    ],
    "env": {
      "LOG_LEVEL": "verbose",
      "API_BASE_URL": "http://localhost:3001",
      "NODE_ENV": "development",
      "PORT": 3001,
      "CACHE_TIMEOUT": 30000,
      "DISABLE_PRICECHECKS": "true"
    }
  },
  "snyk": true
}
